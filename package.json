{
  "private": true,
  "scripts": {
    "dev": "concurrently 'npm run webpack-dev-server' 'npm run express-server'",
    "prod": "webpack --env=prod; git add .; git commit -m \"bundle assets\"",
    "express-server": "nodemon server/app.js -w server -w shared --exec babel-node --presets @babel/preset-env --plugins @babel/plugin-proposal-object-rest-spread",
    "webpack-dev-server": "webpack-dev-server --env=dev --config webpack.config.dev.js"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "clone": "^2.1.2",
    "history": "^4.10.1",
    "js-cookie": "^2.2.1",
    "keymaster": "^1.6.2",
    "knuth-shuffle": "^1.0.8",
    "knuth-shuffle-seeded": "^1.0.6",
    "moment": "^2.24.0",
    "object.entries": "^1.1.0",
    "object.values": "^1.1.0",
    "promise.prototype.finally": "^3.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-router-dom": "^5.0.1",
    "react-router-redux": "5.0.0-alpha.6",
    "react-tap-event-plugin": "3.0.3",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.6.1",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/preset-env": "7.6.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^3.2.0",
    "eslint": "6.4.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.14.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.12.0",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "webpack": "4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.0.0"
  }
